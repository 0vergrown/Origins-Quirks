{
    "name": "Frostbite",
    "description": "This is a status effect from Frost",
    "hidden": true,
    "type": "origins:multiple",
    "resource": {
        "type": "origins:resource",
        "min": 0,
        "max": 180,
        "start_value": 180,
        "min_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "power remove @s powert:mha/frost/frostbite"
                },
                {
                    "type": "origins:execute_command",
                    "command": "stopsound @s player selenivee:quirk.frost.frostbite"
                }
            ]

        },
        "hud_render": {
            "should_render": false
        }
    },
    "snowflake": {
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:execute_command",
            "command": "particle minecraft:snowflake ^ ^2 ^ 0.3 0.3 0.3 0 6 normal"
        }
    },
    "autoremove": {
        "type": "origins:action_over_time",
        "interval": 10,
        "condition": {
            "type": "origins:resource",
            "resource": "powert:mha/frost/frostbite_resource",
            "compare_to": 1,
            "comparison": ">="
        },
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "powert:mha/frost/frostbite_resource",
            "change": -10,
            "operation": "add"
        }
    },
    "effect": {
        "type": "origins:stacking_status_effect",
        "min_stacks": 0,
        "max_stacks": 1,
        "duration_per_stack": 20,
        "tick_rate": 20,
        "effect": {
            "effect": "minecraft:slowness",
            "amplifier": 2,
            "duration": 20,
            "show_icon": false,
            "show_particles": false
        }
    },
    "freezing": {
        "type": "origins:freeze"
    },
    "color": {
        "type": "origins:model_color",
        "alpha": 0.5,
        "red": 0.78,
        "green": 0.882,
        "blue": 0.973
    },
    "glow": {
        "type": "origins:self_glow",
        "red": 0.976,
        "green": 0.988,
        "blue": 1,
        "use_teams": false,
        "bientity_condition": {
            "type": "origins:can_see"
        }
    },
    "shake": {
        "type": "origins:shaking"
    },
    "sound": {
        "type": "origins:action_on_callback",
        "entity_action_gained": {
            "type": "origins:execute_command",
            "command": "playsound selenivee:quirk.frost.frostbite player @s ~ ~ ~ 1 1"
        },
        "entity_action_respawned": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "stopsound @s player selenivee:quirk.frost.frostbite"
                },
                {
                    "type": "origins:execute_command",
                    "command": "power remove @s powert:mha/frost/frostbite"
                }
            ]
        }
    }
}
{
   "badges":[
      {
         "type":"origins:tooltip",
         "sprite":"origins:textures/gui/badge/toggle.png",
         "text":{
            "translate":"badge.quirks.all_for_one.air_walk_toggle",
            "with":[
               {
                  "keybind":"key.origins.quinary_active"
               }
            ]
         }
      },
      {
         "type":"origins:tooltip",
         "sprite":"origins:textures/gui/badge/info.png",
         "text":{
            "translate":"badge.quirks.all_for_one.air_walk_controls",
            "with":[
               {
                  "keybind":"key.jump"
               },
               {
                  "keybind":"key.sneak"
               }
            ]
         }
      }
   ],
    "type":"origins:multiple",
    "toggle":{
       "type":"origins:toggle",
       "active_by_default":false,
       "key":{
          "key":"key.origins.quinary_active",
          "continuous":false
       },
       "retain_state":false,
       "condition":{
         "type": "origins:and",
         "conditions": [
            {
               "type":"origins:resource",
               "resource":"powert:mha/all_for_one/stamina_resource",
               "comparison":">=",
               "compare_to":5
            },
            {
               "type": "origins:resource",
               "resource": "powert:mha/universal/quirk_erased",
               "comparison": "==",
               "compare_to": 0
            }
         ]
       }
    },
    "climb_up":{
       "type":"origins:climbing",
       "allow_holding":false,
       "condition":{
          "type":"origins:and",
          "conditions":[
             {
                "type":"origins:power_active",
                "power":"powert:mha/all_for_one/air_walk_toggle"
             },
             {
                "type":"origins:sneaking",
                "inverted":true
             }
          ]
       }
    },
    "hover":{
       "type":"origins:action_over_time",
       "entity_action":{
          "type":"origins:apply_effect",
          "effect":{
             "effect":"minecraft:levitation",
             "duration":10,
             "amplifier":-1,
             "is_ambient":true,
             "show_particles":false,
             "show_icon":false
          }
       },
       "falling_action":{
          "type":"origins:clear_effect",
          "effect":"minecraft:levitation"
       },
       "interval":5,
       "condition":{
          "type":"origins:and",
          "conditions":[
             {
                "type":"origins:power_active",
                "power":"powert:mha/all_for_one/air_walk_toggle"
             },
             {
                "type":"origins:sneaking",
                "inverted":true
             },
             {
                "type":"origins:on_block",
                "inverted":true
             }
          ]
       }
    },
    "hover_down":{
       "type":"origins:action_over_time",
       "entity_action":{
          "type":"origins:apply_effect",
          "effect":{
             "effect":"minecraft:slow_falling",
             "duration":10,
             "amplifier":0,
             "is_ambient":true,
             "show_particles":false,
             "show_icon":false
          }
       },
       "falling_action":{
          "type":"origins:clear_effect",
          "effect":"minecraft:slow_falling"
       },
       "interval":5,
       "condition":{
          "type":"origins:and",
          "conditions":[
             {
                "type":"origins:power_active",
                "power":"powert:mha/all_for_one/air_walk_toggle"
             },
             {
                "type":"origins:sprinting",
                "inverted":true
             },
             {
                "type":"origins:sneaking",
                "inverted":false
             },
             {
                "type":"origins:on_block",
                "inverted":true
             }
          ]
       }
    },
    "stamina_drain":{
       "type":"origins:action_over_time",
       "entity_action":{
          "type":"origins:modify_resource",
          "modifier":{
             "operation":"add_base_early",
             "value":-5.00
          },
          "resource":"powert:mha/all_for_one/stamina_resource"
       },
       "interval":20,
       "condition":{
          "type":"origins:and",
          "conditions":[
             {
                "type":"origins:power_active",
                "power":"powert:mha/all_for_one/air_walk_toggle"
             },
             {
                "type":"origins:on_block",
                "inverted":true
             }
          ]
       }
    }
}
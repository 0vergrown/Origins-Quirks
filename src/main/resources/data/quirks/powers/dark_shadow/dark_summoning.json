{
   "type":"origins:multiple",
   "callback":{
      "type":"origins:action_on_callback",
      "entity_action_chosen":{
         "type":"origins:and",
         "actions":[
            {
               "type": "origins:execute_command",
               "command": "superpower add powert:dark_shadow_display @s"
            },
            {
               "type":"origins:modify_resource",
               "modifier":{
                  "operation":"set_total",
                  "value":0.00
               },
               "resource":"quirks:dark_shadow/dark_summoning_resource"
            },
            {
               "type":"origins:modify_resource",
               "modifier":{
                  "operation":"set_total",
                  "value":0.00
               },
               "resource":"quirks:dark_shadow/black_abyss_resource"
            },
            {
               "type":"origins:modify_resource",
               "modifier":{
                  "operation":"set_total",
                  "value":0.00
               },
               "resource":"quirks:dark_shadow/womb_resource"
            }
         ]
      },
      "entity_action_lost": {
         "type":"origins:execute_command",
         "command":"power revokeall @s quirks:dark_shadow"
      },
      "entity_action_removed":{
         "type":"origins:and",
         "actions":[
            {
               "type":"origins:modify_resource",
               "modifier":{
                  "operation":"set_total",
                  "value":0.00
               },
               "resource":"quirks:dark_shadow/dark_summoning_resource"
            },
            {
               "type":"origins:modify_resource",
               "modifier":{
                  "operation":"set_total",
                  "value":0.00
               },
               "resource":"quirks:dark_shadow/black_abyss_resource"
            },
            {
               "type":"origins:modify_resource",
               "modifier":{
                  "operation":"set_total",
                  "value":0.00
               },
               "resource":"quirks:dark_shadow/womb_resource"
            }
         ]
      },
      "entity_action_respawned":{
         "type":"origins:and",
         "actions":[
            {
               "type": "origins:execute_command",
               "command": "superpower add powert:dark_shadow_display @s"
            },
            {
               "type":"origins:modify_resource",
               "modifier":{
                  "operation":"set_total",
                  "value":0.00
               },
               "resource":"quirks:dark_shadow/dark_summoning_resource"
            },
            {
               "type":"origins:modify_resource",
               "modifier":{
                  "operation":"set_total",
                  "value":0.00
               },
               "resource":"quirks:dark_shadow/black_abyss_resource"
            },
            {
               "type":"origins:modify_resource",
               "modifier":{
                  "operation":"set_total",
                  "value":0.00
               },
               "resource":"quirks:dark_shadow/womb_resource"
            }
         ]
      },
      "execute_chosen_when_orb":true
   },
   "resource":{
      "type": "origins:resource",
      "min": 0,
      "max": 1,
      "hud_render": {
          "should_render": false
      },
      "start_value": 0
  },
   "spawn":{
      "type":"origins:active_self",
      "entity_action":{
         "type":"origins:if_else",
         "condition":{
            "type": "origins:resource",
            "resource":"quirks:dark_shadow/dark_summoning_resource",
            "comparison": "==",
            "compare_to": 1
         },
         "if_action":{
            "type":"origins:and",
            "actions":[
               {
                  "type":"origins:spawn_particles",
                  "particle":{
                     "type":"minecraft:dust_color_transition",
                     "params":"0 0 0 1 0.36 0.11 0.41"
                  },
                  "count":50,
                  "speed":0.15,
                  "force":true,
                  "spread":{
                     "x":0.40,
                     "y":0.40,
                     "z":0.40
                  },
                  "offset_y":0.50
               },
               {
                  "type":"origins:modify_resource",
                  "modifier":{
                     "operation":"set_total",
                     "value":0.00
                  },
                  "resource":"quirks:dark_shadow/dark_summoning_resource"
               }
            ]
         },
         "else_action":{
            "type":"origins:and",
            "actions":[
               {
                  "type":"origins:modify_resource",
                  "modifier":{
                     "operation":"set_total",
                     "value":1.00
                  },
                  "resource":"quirks:dark_shadow/dark_summoning_resource"
               },
               {
                  "type":"origins:spawn_particles",
                  "particle":{
                     "type":"minecraft:dust_color_transition",
                     "params":"0 0 0 1 0.36 0.11 0.41"
                  },
                  "count":50,
                  "speed":0.15,
                  "force":true,
                  "spread":{
                     "x":0.40,
                     "y":0.40,
                     "z":0.40
                  },
                  "offset_y":0.50
               },
               {
                  "type":"origins:play_sound",
                  "sound":"minecraft:entity.wither.spawn",
                  "volume":0.50,
                  "pitch":2.00
               }
            ]
         }
      },
      "cooldown":1,
      "hud_render":{
         "should_render":false
      },
      "key":{
         "key":"key.quirks.primary",
         "continuous":false
      },
      "condition":{
         "type":"origins:and",
         "conditions":[
            {
               "type":"origins:resource",
               "resource":"quirks:dark_shadow/black_abyss_resource",
               "comparison":"==",
               "compare_to":0
            },
            {
               "type":"origins:resource",
               "resource":"quirks:dark_shadow/womb_resource",
               "comparison":"==",
               "compare_to":0
            },
            {
               "type": "origins:resource",
               "resource": "quirks:universal/quirk_erased",
               "comparison": "==",
               "compare_to": 0
            }
         ]
      }
   }
}
{
   "type":"origins:multiple",
   "timer":{
      "type":"origins:resource",
      "min":0,
      "max":70,
      "hud_render":{
         "should_render":false
      },
      "start_value":70,
      "min_action":{
         "type":"origins:and",
         "actions":[
            {
               "type":"origins:revoke_power",
               "power":"powert:mha/dark_shadow/womb_effects",
               "source":"powert:mha/dark_shadow/womb_effects"
            },
            {
               "type":"origins:play_sound",
               "sound":"minecraft:block.soul_soil.step",
               "volume":1.00,
               "pitch":0.5
            },
            {
               "type":"origins:play_sound",
               "sound":"minecraft:entity.phantom.hurt",
               "volume":0.33,
               "pitch":0.50
            }
         ]
      }
   },
   "tick_down":{
      "type":"origins:action_over_time",
      "entity_action":{
         "type":"origins:and",
         "actions":[
            {
               "type":"origins:execute_command",
               "command":"execute at @s positioned ~ ~0.9 ~ run function powert:mha/dark_shadow/womb_particles"
            },
            {
               "type":"origins:modify_resource",
               "modifier":{
                  "operation":"add_base_early",
                  "value":-1.00
               },
               "resource":"powert:mha/dark_shadow/womb_effects_timer"
            },
            {
               "type": "origins:execute_command",
               "command": "title @s actionbar {\"translate\":\"actionbar.quirks.dark_shadow.womb_escape_message\",\"color\":\"dark_purple\",\"bold\":false,\"italic\":true,\"underlined\":false,\"strikethrough\":false,\"obfuscated\":false}"
            }
         ]
      },
      "interval":1
   },
   "stunned":{
      "type":"origins:attribute",
      "modifiers":[
         {
            "attribute":"minecraft:generic.movement_speed",
            "operation":"multiply_total",
            "value":-1,
            "name": "Quirks: Dark Shadow Womb Disable Movement"
         },
         {
            "attribute":"minecraft:generic.knockback_resistance",
            "operation":"multiply_total",
            "value":0.33,
            "name": "Quirks: Dark Shadow Womb Disable Knockback"
         }
      ],
      "update_health":false
   },
   "disable_air_speed":{
      "type":"origins:modify_air_speed",
      "modifier":{
         "operation":"multiply_total",
         "value":-1
      }
   },
   "disable_jump":{
      "type":"origins:modify_jump",
      "modifier":{
         "operation":"multiply_total",
         "value":-1,
         "name": "Quirks: Dark Shadow Womb Disable Jump"
      }
   },
   "disable_damage":{
      "type":"origins:modify_damage_dealt",
      "modifier":{
         "operation":"multiply_total",
         "value":-1,
         "name": "Quirks: Dark Shadow Womb Disable Dealing Damage"
      }
   },
   "disable_sprint":{
      "type":"origins:prevent_sprinting"
   },
   "prevent_item_use":{
      "type":"origins:prevent_item_use"
   },
   "prevent_entity_use":{
      "type":"origins:prevent_entity_use"
   },
   "no_touch_block":{
      "type":"origins:prevent_block_selection"
   },
   "break_free":{
      "type":"origins:active_self",
      "entity_action":{
         "type":"origins:and",
         "actions":[
            {
               "type":"origins:modify_resource",
               "modifier":{
                  "operation":"add_base_early",
                  "value":-4.00
               },
               "resource":"powert:mha/dark_shadow/womb_effects_timer"
            },
            {
               "type":"origins:play_sound",
               "sound":"minecraft:entity.phantom.hurt",
               "volume":0.33,
               "pitch":0.50
            }
         ]
      },
      "cooldown":2,
      "hud_render": {
         "should_render": false
      },
      "key":{
         "key":"key.jump",
         "continuous": false
      }
   }
}
{
   "badges":[
      {
         "type":"origins:tooltip",
         "sprite":"origins:textures/gui/badge/active.png",
         "text":{
            "translate":"badge.quirks.fa_jin.kinetic_energy",
            "with":[
               {
                  "keybind":"key.saveToolbarActivator"
               },
               {
                  "keybind":"key.loadToolbarActivator"
               }
            ]
         }
      }
   ],
   "type":"origins:multiple",
   "resource":{
      "type":"origins:resource",
      "min":0,
      "max":50,
      "hud_render":{
         "should_render":true,
         "sprite_location":"powert:textures/gui/sprites/fa_jin/resource_bar_01.png",
         "bar_index":0
      },
      "start_value":0
   },
   "charged_particles":{
      "type":"origins:action_over_time",
      "entity_action":{
         "type":"origins:and",
         "actions":[
            {
               "type": "origins:spawn_particles",
               "particle": {
                   "type": "minecraft:dust",
                   "params": "0.84 0.06 0.06 1"
               },
               "count": 10,
               "speed": 0.00,
               "force": false,
               "spread": {
                  "x": 0.15,
                  "y": 0.10,
                  "z": 0.15
               },
               "offset_x": 0.00,
               "offset_y": 0.20,
               "offset_z": 0.00
            },
            {
               "type": "origins:spawn_particles",
               "particle": {
                   "type": "minecraft:dust",
                   "params": "0 0 0 1"
               },
               "count": 10,
               "speed": 0.00,
               "force": false,
               "spread": {
                  "x": 0.10,
                  "y": 0.10,
                  "z": 0.10
               },
               "offset_x": 0.00,
               "offset_y": 0.20,
               "offset_z": 0.00
            }
         ]
      },
      "interval":1,
      "condition":{
         "type":"origins:resource",
         "resource":"quirks:fa_jin/kinetic_energy_resource",
         "comparison":">=",
         "compare_to":10
      }
   },
   "power_output":{
      "type":"origins:resource",
      "min":0,
      "max":5,
      "hud_render":{
         "should_render":false
      },
      "start_value":0
   },
   "select_power_output_up":{
      "type":"origins:active_self",
      "entity_action":{
         "type":"origins:if_else",
         "condition":{
            "type":"origins:resource",
            "resource":"quirks:fa_jin/kinetic_energy_power_output",
            "comparison":"!=",
            "compare_to":5
         },
         "if_action":{
            "type":"origins:modify_resource",
            "modifier":{
               "operation":"add_base_early",
               "value":1.00
            },
            "resource":"quirks:fa_jin/kinetic_energy_power_output"
         },
         "else_action":{
            "type":"origins:modify_resource",
            "modifier":{
               "operation":"set_total",
               "value":0.00
            },
            "resource":"quirks:fa_jin/kinetic_energy_power_output"
         }
      },
      "cooldown":1,
      "hud_render":{
         "should_render":false
      },
      "key":{
         "key":"key.saveToolbarActivator",
         "continuous":false
      }
   },
   "select_power_output_down":{
      "type":"origins:active_self",
      "entity_action":{
         "type":"origins:if_else",
         "condition":{
            "type":"origins:resource",
            "resource":"quirks:fa_jin/kinetic_energy_power_output",
            "comparison":"!=",
            "compare_to":0
         },
         "if_action":{
            "type":"origins:modify_resource",
            "modifier":{
               "operation":"add_base_early",
               "value":-1.00
            },
            "resource":"quirks:fa_jin/kinetic_energy_power_output"
         },
         "else_action":{
            "type":"origins:modify_resource",
            "modifier":{
               "operation":"set_total",
               "value":5.00
            },
            "resource":"quirks:fa_jin/kinetic_energy_power_output"
         }
      },
      "cooldown":1,
      "hud_render":{
         "should_render":false
      },
      "key":{
         "key":"key.loadToolbarActivator",
         "continuous":false
      }
   },
   "output_message_level_0":{
      "type":"origins:action_over_time",
      "rising_action":{
         "type":"origins:execute_command",
         "command":"title @s actionbar {\"bold\":false,\"color\":\"white\",\"italic\":false,\"obfuscated\":false,\"strikethrough\":false,\"text\":\"0%\",\"underlined\":false}"
      },
      "interval":1,
      "condition":{
         "type":"origins:resource",
         "resource":"quirks:fa_jin/kinetic_energy_power_output",
         "comparison":"==",
         "compare_to":0
      }
   },
   "output_message_level_1":{
      "type":"origins:action_over_time",
      "rising_action":{
         "type":"origins:execute_command",
         "command":"title @s actionbar {\"bold\":false,\"color\":\"#66ffcc\",\"italic\":false,\"obfuscated\":false,\"strikethrough\":false,\"text\":\"20%\",\"underlined\":false}"
      },

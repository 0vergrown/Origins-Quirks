{
   "badges":[
      {
         "type":"origins:tooltip",
         "sprite":"origins:textures/gui/badge/active.png",
         "text":{
            "translate":"badge.quirks.zero_gravity.gunhead_martial_arts",
            "with":[
               {
                  "keybind":"key.quirks.primary"
               },
               {
                  "keybind":"key.quirks.ternary"
               }
            ]
         }
      }
   ],
   "type":"origins:multiple",
   "resource":{
      "type":"origins:resource",
      "min":0,
      "max":1,
      "hud_render":{
         "should_render":false
      },
      "start_value":0
   },
   "cycle":{
      "type":"apoli:active_self",
      "entity_action":{
         "type":"apoli:if_else",
         "condition":{
            "type":"origins:resource",
            "resource":"quirks:zero_gravity/gunhead_martial_arts_resource",
            "comparison":"==",
            "compare_to":0
         },
         "if_action":{
            "type":"apoli:modify_resource",
            "modifier":{
               "operation":"set_total",
               "value":1
            },
            "resource":"quirks:zero_gravity/gunhead_martial_arts_resource"
         },
         "else_action":{
            "type":"apoli:modify_resource",
            "modifier":{
               "operation":"set_total",
               "value":0
            },
            "resource":"quirks:zero_gravity/gunhead_martial_arts_resource"
         }
      },
      "cooldown":1,
      "hud_render":{
         "should_render":false
      },
      "key":{
         "key":"key.quirks.primary",
         "continuous":false
      }
   },
   "selected_mode_attack_message":{
      "type":"origins:action_over_time",
      "rising_action":{
         "type":"origins:execute_command",
         "command":"title @s actionbar {\"translate\":\"actionbar.quirks.zero_gravity.gunhead_martial_arts_selected_mode_attack.message\",\"bold\":false,\"italic\":false,\"underlined\":false,\"strikethrough\":false,\"obfuscated\":false}"
      },
      "interval":1,
      "condition":{
         "type":"origins:resource",

{
    "type":"origins:multiple",
    "toggle":{
       "type":"origins:toggle",
       "active_by_default":false,
       "key":{
          "key":"key.origins.secondary_active"
       },
       "retain_state":false
    },
    "floating":{
       "type":"origins:action_over_time",
       "entity_action":{
          "type":"origins:apply_effect",
          "effect":{
             "effect":"minecraft:levitation",
             "duration":400,
             "amplifier":1,
             "is_ambient":false,
             "show_particles":false,
             "show_icon":true
          }
       },
       "rising_action": {
         "type": "origins:execute_command",
            "command": "playsound quirks:zero_gravity.float player @a[distance=0..16] ~ ~ ~ 1.00 1.00 1.00"
       },
       "falling_action":{
          "type":"origins:if_else",
          "condition":{
             "type":"origins:on_block",
             "inverted":true
          },
          "if_action":{
             "type":"origins:and",
             "actions":[
                {
                   "type":"origins:clear_effect",
                   "effect":"minecraft:levitation"
                },
                {
                   "type":"origins:apply_effect",
                   "effect":{
                      "effect":"minecraft:slow_falling",
                      "duration":40,
                      "amplifier":0,
                      "is_ambient":false,
                      "show_particles":false,
                      "show_icon":true
                   }
                }
             ]
          },
          "else_action":{
             "type":"origins:clear_effect",
             "effect":"minecraft:levitation"
          }
       },
       "interval":20,
       "condition":{
          "type":"origins:power_active",
          "power":"quirks:zero_gravity/floating_toggle",
          "inverted":false
       }
    }
       }
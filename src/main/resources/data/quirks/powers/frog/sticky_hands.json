{
    "type":"origins:multiple",
    "toggle":{
       "type":"origins:toggle",
       "active_by_default":false,
       "key":{
          "key":"key.quirks.ternary",
          "continuous":false
       },
       "retain_state":false,
       "condition": {
         "type": "origins:resource",
         "resource": "quirks:universal/quirk_erased",
         "comparison": "==",
         "compare_to": 0
       }
    },
    "climbing":{
       "type":"origins:climbing",
       "allow_holding":true,
       "hold_condition":{
          "type":"origins:and",
          "conditions":[
             {
                "type":"origins:or",
                "conditions":[
                   {
                      "type":"origins:block_collision",
                      "offset_x":0.01,
                      "offset_y":0,
                      "offset_z":0.01
                   },
                   {
                      "type":"origins:block_collision",
                      "offset_x":-0.01,
                      "offset_y":0,
                      "offset_z":-0.01
                   }
                ]
             }
          ]
       },
       "condition":{
          "type":"origins:and",
          "conditions":[
             {
                "type":"origins:power_active",
                "power":"quirks:frog/sticky_hands_toggle"
             },
             {
                "type":"origins:collided_horizontally"
             },
             {
                "type":"origins:resource",
                "resource":"quirks:frog/stamina_resource",
                "comparison":">=",
                "compare_to":5
             }
          ]
       }
    },
    "stamina_drain":{
       "type":"origins:action_over_time",
       "entity_action":{
          "type":"origins:if_else",
          "condition":{
             "type":"origins:resource",
             "resource":"quirks:frog/stamina_resource",
             "comparison":">=",
             "compare_to":5
          },
          "if_action":{
             "type":"origins:change_resource",
             "resource":"quirks:frog/stamina_resource",
             "change":-5,
             "operation":"add"
          },
          "else_action":{
             "type":"origins:and",
             "actions":[
                {
                   "type":"origins:play_sound",
                   "sound":"minecraft:entity.frog.ambient",
                   "volume":1.00,
                   "pitch":1.00
                },
                {
                   "type":"origins:toggle",
                   "power":"quirks:frog/sticky_hands_toggle"
                }
             ]
          }
       },
       "interval":20,
       "condition":{
          "type":"origins:and",
          "conditions":[
             {
                "type":"origins:power_active",
                "power":"quirks:frog/sticky_hands_toggle"
             },
             {
                "type":"origins:or",
                "conditions":[
                   {
                      "type":"origins:block_collision",
                      "offset_x":0.01,
                      "offset_y":0.00,
                      "offset_z":0.01
                   },
                   {
                      "type":"origins:block_collision",
                      "offset_x":-0.01,
                      "offset_y":0.00,
                      "offset_z":-0.01
                   }
                ]
             }
          ]
       }
    }
}
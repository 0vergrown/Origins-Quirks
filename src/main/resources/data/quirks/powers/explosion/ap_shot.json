         "else_action":{
            "type":"origins:modify_resource",
            "modifier":{
               "operation":"set_total",
               "value":0.00
            },
            "resource":"quirks:explosion/ap_shot_mode"
         }
      },
      "cooldown":1,
      "hud_render":{
         "should_render":false
      },
      "key":{
         "key":"key.saveToolbarActivator",
         "continuous":false
      }
   },
   "mode_selector_down":{

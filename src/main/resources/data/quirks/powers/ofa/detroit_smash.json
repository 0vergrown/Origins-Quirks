{
    "name":"St. Louis Smash",
    "description":"You arch your leg back and perform a strong roundhouse kick.",
    "hidden":false,
    "type":"origins:multiple",
    "resource":{
       "type":"origins:resource",
       "min":0,
       "max":1,
       "start_value":0,
       "hud_render":{
          "should_render":false,
          "sprite_location":"origins:textures/gui/resource_bar.png",
          "bar_index":7
       }
    },
    "cooldown":{
       "type":"origins:resource",
       "min":0,
       "max":200,
       "start_value":0,
       "hud_render":{
          "should_render":true,
          "sprite_location":"origins:textures/gui/resource_bar.png",
          "bar_index":8,
          "condition":{
             "inverted":false,
             "type":"origins:resource",
             "resource":"*:*_cooldown",
             "comparison":"<",
             "compare_to":200
          }
       }
    },
    "drain":{
       "type":"origins:action_over_time",
       "interval":1,
       "entity_action":{
          "type":"origins:change_resource",
          "resource":"*:*_cooldown",
          "change":1,
          "operation":"add"
       }
    },
    "toggle":{
       "type":"origins:active_self",
       "entity_action":{
          "type":"origins:if_else",
          "condition":{
             "type":"origins:resource",
             "resource":"*:*_resource",
             "comparison":"==",
             "compare_to":1
          },
          "if_action":{
             "type":"origins:and",
             "actions":[
                {
                   "type":"origins:change_resource",
                   "resource":"*:*_resource",
                   "change":-1,
                   "operation":"add"
                },
                {
                   "type":"origins:execute_command",
                   "command":"/title @s actionbar [{\"text\":\"St. Louis Smash disabled\",\"color\":\"white\"}]",
                   "permission_level":4
                }
             ]
          },
          "else_action":{
             "type":"origins:and",
             "actions":[
                {
                   "type":"origins:change_resource",
                   "resource":"*:*_resource",
                   "change":1,
                   "operation":"add"
                },
                {
                   "type":"origins:execute_command",
                   "command":"title @s actionbar [{\"text\":\"St. Louis Smash active\",\"color\":\"white\"}]"
                }
             ]
          }
       },
       "cooldown":1,
       "hud_render":{
          "should_render":false
       },
       "key":{
          "key":"key.origins.primary_active",
          "continuous": false
       },
       "condition":{
        "type":"origins:and",
        "conditions":[
           {
              "type":"origins:resource",
              "resource":"*:*_cooldown",
              "comparison":"==",
              "compare_to":200
           },
           {
              "type":"origins:resource",
              "resource":"powert:allpowers/staminabar_bar",
              "comparison":">=",
              "compare_to":200
           },
           {
              "type":"origins:resource",
              "resource":"powert:mha/universal/quirk_erased",
              "comparison":"==",
              "compare_to":0
           }
        ]
     }
    },
    "sound":{
       "condition":{
          "inverted":false,
          "type":"origins:resource",
          "resource":"*:*_resource",
          "comparison":"==",
          "compare_to":1
       },
       "type":"origins:action_over_time",
       "interval":5,
       "entity_action":{
          "type":"origins:and",
          "actions":[
             {
                "type":"origins:play_sound",
                "sound":"minecraft:entity.player.breath",
                "volume":2,
                "pitch":1.8
             }
          ]
       }
    },
    "toggle_particle":{
       "condition":{
          "inverted":false,
          "type":"origins:resource",
          "resource":"*:*_resource",
          "comparison":"==",
          "compare_to":1
       },
       "type":"origins:action_over_time",
       "interval":1,
       "entity_action":{
          "type":"origins:and",
          "actions":[
             {
                "type":"origins:execute_command",
                "permission_level":4,
                "command":"/particle minecraft:dust 1 1 1 1 ~ ~0.3 ~0.1 0.3 0.2 0.1 0.1 3 force"
             },
             {
                "type":"origins:play_sound",
                "sound":"minecraft:entity.player.breath",
                "volume":2,
                "pitch":2
             },
             {
                "type":"origins:change_resource",
                "resource":"powert:allpowers/staminabar_bar",
                "change":-3,
                "operation":"add"
             },
             {
                "type":"origins:if_else",
                "condition":{
                   "type":"origins:resource",
                   "resource":"powert:allpowers/ofa/fullcowling_resource",
                   "comparison":">=",
                   "compare_to":1
                },
                "if_action":{
                   "type":"origins:nothing"
                },
                "else_action":{
                   "type":"origins:damage",
                   "source":{
                      "name":"cowling"
                   },
                   "amount":4,
                   "bypasses_armor":true
                }
             },
             {
                "type":"origins:execute_command",
                "command":"/particle minecraft:happy_villager ~-0.1 ~0.2 ~0 0.1 0.2 0.1 1 2 force @a"
             }
          ]
       }
    },
    "resourcecheck":{
       "type":"origins:action_over_time",
       "entity_action":{
          "type":"origins:if_else_list",
          "actions":[
             {
                "condition":{
                   "type":"origins:and",
                   "conditions":[
                      {
                         "type":"origins:resource",
                         "resource":"powert:allpowers/staminabar_bar",
                         "comparison":"<=",
                         "compare_to":200
                      },
                      {
                         "inverted":false,
                         "type":"origins:resource",
                         "resource":"*:*_resource",
                         "comparison":"==",
                         "compare_to":1
                      }
                   ]
                },
                "action":{
                   "type":"origins:and",
                   "actions":[
                      {
                         "type":"origins:change_resource",
                         "resource":"*:*_resource",
                         "change":-1,
                         "operation":"add"
                      },
                      {
                         "type":"origins:execute_command",
                         "command":"/title @s actionbar [{\"text\":\"St. Louis Smash disabled\",\"color\":\"white\"}]",
                         "permission_level":4
                      }
                   ]
                }
             }
          ]
       },
       "falling_action": {
        "type":"origins:and",
        "actions":[
           {
              "type":"origins:change_resource",
              "resource":"*:*_resource",
              "change":-1,
              "operation":"add"
           },
           {
              "type":"origins:execute_command",
              "command":"/title @s actionbar [{\"text\":\"St. Louis Smash disabled\",\"color\":\"white\"}]",
              "permission_level":4
           }
        ]
       },
       "interval":2,
       "condition":{
          "type":"origins:resource",
          "resource":"powert:mha/universal/quirk_erased",
          "comparison":"==",
          "compare_to":0
       }
    },
    "modify_damage":{
       "condition":{
          "inverted":false,
          "type":"origins:resource",
          "resource":"*:*_resource",
          "comparison":"==",
          "compare_to":1
       },
       "type":"origins:modify_damage_dealt",
       "modifier":{
          "operation":"addition",
          "value":15
       },
       "self_action":{
          "type":"origins:and",
          "actions":[
             {
                "type":"origins:change_resource",
                "resource":"*:*_resource",
                "change":-1,
                "operation":"add"
             },
             {
                "type":"origins:change_resource",
                "resource":"*:*_cooldown",
                "change":0,
                "operation":"set"
             },
             {
                "type":"origins:change_resource",
                "resource":"powert:allpowers/staminabar_bar",
                "change":-200,
                "operation":"add"
             }
          ]
       },
       "target_action":{
          "type":"origins:and",
          "actions":[
             {
                "type":"origins:execute_command",
                "permission_level":4,
                "command":"playsound minecraft:entity.generic.explode player @a[distance=..10] ~ ~1 ~ 1 0"
             },
             {
                "type":"origins:execute_command",
                "permission_level":4,
                "command":"playsound minecraft:entity.generic.explode player @a[distance=..10] ~ ~1 ~ 1 1"
             },
             {
                "type":"origins:execute_command",
                "permission_level":4,
                "command":"playsound minecraft:entity.generic.explode player @a[distance=..10] ~ ~1 ~ 1 2"
             },
             {
                "type":"origins:execute_command",
                "permission_level":4,
                "command":"/particle minecraft:sweep_attack ~ ~0.2 ~0.2 0.3 0.3 0.3 0 2 normal"
             }
          ]
       }
    }
}
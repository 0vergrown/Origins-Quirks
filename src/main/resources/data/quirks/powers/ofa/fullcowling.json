{
    "name":"Full Cowling",
    "description":"Control your output of power.",
    "badges":[
       {
          "type":"origins:tooltip",
          "sprite":"origins:textures/gui/badge/active.png",
          "text":{
             "translate":"badge.quirks.one_for_all.full_cowling",
             "with":[
                {
                   "keybind":"key.saveToolbarActivator"
                },
                {
                   "keybind":"key.loadToolbarActivator"
                }
             ]
          }
       }
    ],
    "type":"origins:multiple",
    "power_output":{
       "type":"origins:resource",
       "min":0,
       "max":10,
       "hud_render":{
          "should_render":false
       },
       "start_value":0
    },
    "stamina_check":{
       "type":"origins:action_over_time",
       "entity_action":{
          "type":"origins:modify_resource",
          "modifier":{
             "operation":"set_total",
             "value":0.00
          },
          "resource":"powert:mha/ofa/fullcowling_power_output"
       },
       "falling_action": {
         "type":"origins:modify_resource",
         "modifier":{
            "operation":"set_total",
            "value":0.00
         },
         "resource":"powert:mha/ofa/fullcowling_power_output"
       },
       "interval":2,
       "condition":{
         "type": "origins:and",
         "conditions": [
            {
               "type":"origins:resource",
               "resource":"powert:allpowers/staminabar_bar",
               "comparison":"<=",
               "compare_to":2
            },
            {
               "type":"origins:resource",
               "resource":"powert:mha/universal/quirk_erased",
               "comparison":"==",
               "compare_to":0
            }
         ]
       }
    },
    "select_power_output_up":{
       "type":"origins:active_self",
       "entity_action":{
          "type":"origins:if_else",
          "condition":{
             "type":"origins:resource",
             "resource":"powert:mha/ofa/fullcowling_power_output",
             "comparison":"!=",
             "compare_to":10
          },
          "if_action":{
             "type":"origins:modify_resource",
             "modifier":{
                "operation":"add_base_early",
                "value":1.00
             },
             "resource":"powert:mha/ofa/fullcowling_power_output"
          },
          "else_action":{
             "type":"origins:modify_resource",
             "modifier":{
                "operation":"set_total",
                "value":0.00
             },
             "resource":"powert:mha/ofa/fullcowling_power_output"
          }
       },
       "cooldown":1,
       "hud_render":{
          "should_render":false
       },
       "key":{
          "key":"key.saveToolbarActivator",
          "continuous":false
       },
       "condition":{
         "type": "origins:and",
         "conditions": [
            {
               "type":"origins:resource",
               "resource":"powert:allpowers/staminabar_bar",
               "comparison":">=",
               "compare_to":2
            },
            {
               "type":"origins:resource",
               "resource":"powert:mha/universal/quirk_erased",
               "comparison":"==",
               "compare_to":0
            }
         ]
       }
    },
    "select_power_output_down":{
       "type":"origins:active_self",
       "entity_action":{
          "type":"origins:if_else",
          "condition":{
             "type":"origins:resource",
             "resource":"powert:mha/ofa/fullcowling_power_output",
             "comparison":"!=",
             "compare_to":0

{
    "type":"origins:multiple",
    "actor_set":{
       "type":"origins:entity_set",
       "action_on_remove":{
          "type":"origins:target_action",
          "action":{
             "type":"origins:execute_command",
             "command":"kill @s"
          }
       }
    },
    "spawn":{
       "type":"origins:active_self",
       "entity_action":{
          "type":"origins:if_else",
          "condition":{
             "type":"origins:entity_set_size",
             "set":"quirks:dark_shadow/dark_summoning_actor_set",
             "comparison":"==",
             "compare_to":0
          },
          "if_action":{
             "type":"origins:and",
             "actions":[
                {
                   "type":"origins:execute_command",
                   "command":"summon minecraft:armor_stand ~ ~ ~ {Silent:1b,Invulnerable:1b,Marker:1b,Invisible:1b,DisabledSlots:4144959,ArmorItems:[{},{},{},{id:\"minecraft:carved_pumpkin\",count:1,components:{\"minecraft:custom_model_data\":385957}}]}"
                },
                {
                   "type":"origins:delay",
                   "action":{
                      "type":"origins:area_of_effect",
                      "radius":2.00,
                      "shape":"cube",
                      "bientity_action":{
                         "type":"origins:add_to_entity_set",
                         "set":"quirks:dark_shadow/dark_summoning_actor_set"
                      },
                      "bientity_condition":{
                         "type":"origins:all_of",
                         "conditions":[
                            {
                               "type":"origins:in_entity_set",
                               "set":"quirks:dark_shadow/dark_summoning_actor_set",
                               "inverted":true
                            },
                            {
                               "type":"origins:target_condition",
                               "condition":{
                                  "type":"origins:entity_type",
                                  "entity_type":"minecraft:armor_stand"
                               }
                            }
                         ]
                      },
                      "include_actor":false
                   },
                   "ticks":1
                }
             ]
          },
          "else_action":{
             "type":"origins:action_on_entity_set",
             "set":"quirks:dark_shadow/dark_summoning_actor_set",
             "bientity_action":{
                "type":"origins:remove_from_entity_set",
                "set":"quirks:dark_shadow/dark_summoning_actor_set"
             },
             "limit":1,
             "reverse":false
          }
       },
       "cooldown":1,
       "hud_render":{
          "should_render":false
       },
       "key":{
          "key":"key.origins.primary_active",
          "continuous":false
       },
       "condition":{
         "type":"origins:resource",
         "resource":"quirks:dark_shadow/move_selector_resource",
         "comparison":"==",
         "compare_to":0
      }
    },
    "position":{
       "type":"origins:action_over_time",
       "entity_action":{
         "type":"origins:and",
         "actions":[
            {
               "type":"origins:execute_command",
               "command":"tp @n[type=minecraft:armor_stand] @s"
            },
            {
               "type":"origins:execute_command",
               "command":"data modify entity @n[type=minecraft:armor_stand,limit=1] Rotation set from entity @s Rotation"
            }
         ]
       },
       "interval":1,
       "condition":{
         "type":"origins:entity_set_size",
         "set":"quirks:dark_shadow/dark_summoning_actor_set",
         "comparison":"==",
         "compare_to":1
      }
    }
}
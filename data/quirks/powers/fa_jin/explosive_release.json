{
   "type":"origins:multiple",
   "resource":{
      "type":"origins:resource",
      "min":0,
      "max":1,
      "hud_render":{
         "should_render":false
      },
      "start_value":0
   },
   "charge":{
      "type":"origins:resource",
      "min":0,
      "max":3,
      "hud_render":{
         "should_render":false
      },
      "start_value":0
   },
   "toggle":{
      "type":"origins:active_self",
      "entity_action":{
         "type":"origins:and",
         "actions":[
            {
               "type":"origins:modify_resource",
               "modifier":{
                  "operation":"set_total",
                  "amount":1.00
               },
               "resource":"quirks:fa_jin/explosive_release_resource"
            },
            {
               "type":"origins:play_sound",
               "sound":"minecraft:block.beacon.power_select",
               "volume":1.00,
               "pitch":2.00
            }
         ]
      },
      "cooldown":1,
      "hud_render":{
         "should_render":false
      },
      "key":{
         "key":"key.origins.primary_active",
         "continuous":false
      },
      "condition":{
         "type":"origins:all_of",
         "conditions":[
            {
               "type":"origins:resource",
               "resource":"quirks:fa_jin/move_selector_resource",
               "comparison":"==",
               "compare_to":3
            },
            {
               "type":"origins:resource",
               "resource":"quirks:fa_jin/kinetic_energy_resource",
               "comparison":"==",
               "compare_to":50
            },
            {
               "type":"origins:resource",
               "resource":"quirks:fa_jin/explosive_release_resource",
               "comparison":"==",
               "compare_to":0
            }
         ]
      }
   },
   "charge_up":{
      "type":"origins:action_over_time",
      "entity_action":{
         "type":"origins:and",
         "actions":[
            {
               "type":"origins:modify_resource",
               "modifier":{
                  "operation":"add_base_early",
                  "amount":1.00
               },
               "resource":"quirks:fa_jin/explosive_release_charge"
            },
            {
               "type":"origins:play_sound",
               "sound":"minecraft:block.beacon.ambient",
               "volume":1.00,
               "pitch":2.00
            },
            {
               "type":"origins:execute_command",
               "command":"function quirks:fa_jin/explosive_release_particles"
            }
         ]
      },
      "interval":40,
      "condition":{
         "type":"origins:all_of",
         "conditions":[
            {
               "type":"origins:resource",
               "resource":"quirks:fa_jin/explosive_release_resource",
               "comparison":"==",
               "compare_to":1
            },
            {
               "type":"origins:resource",
               "resource":"quirks:fa_jin/explosive_release_charge",
               "comparison":"!=",
               "compare_to":3
            }
         ]
      }
   },
   "active":{
      "type":"origins:action_over_time",
      "rising_action":{
         "type":"origins:and",
         "actions":[
            {
               "type":"origins:explode",
               "power":5,
               "destruction_type":"destroy_with_decay",
               "damage_self":false,
               "create_fire":false
            },
            {
               "type":"origins:spawn_particles",
               "particle":{
                  "type":"minecraft:dust",
                  "params":"{color:[0.84,0.06,0.06],scale:1}"
               },
               "count":500,
               "speed":0.50,
               "force":true,
               "spread":{
                  "x":0.50,
                  "y":0.50,
                  "z":0.50
               },
               "offset_x":0.00,
               "offset_y":0.30,
               "offset_z":0.00
            },
            {
               "type":"origins:spawn_particles",
               "particle":{
                  "type":"minecraft:dust",
                  "params":"{color:[0.00,0.00,0.00],scale:1}"
               },
               "count":500,
               "speed":0.50,
               "force":true,
               "spread":{
                  "x":0.50,
                  "y":0.50,
                  "z":0.50
               },
               "offset_x":0.00,
               "offset_y":0.30,
               "offset_z":0.00
            },
            {
               "type":"origins:modify_resource",
               "modifier":{
                  "operation":"set_total",
                  "amount":0.00
               },
               "resource":"quirks:fa_jin/kinetic_energy_resource"
            },
            {
               "type":"origins:modify_resource",
               "modifier":{
                  "operation":"set_total",
                  "amount":0.00
               },
               "resource":"quirks:fa_jin/explosive_release_charge"
            },
            {
               "type":"origins:modify_resource",
               "modifier":{
                  "operation":"set_total",
                  "amount":0.00
               },
               "resource":"quirks:fa_jin/explosive_release_resource"
            }
         ]
      },
      "interval":1,
      "condition":{
         "type":"origins:resource",
         "resource":"quirks:fa_jin/explosive_release_charge",
         "comparison":"==",
         "compare_to":3
      }
   },
   "disabled_movement":{
      "type":"origins:conditioned_attribute",
      "modifier":{
         "attribute":"minecraft:generic.movement_speed",
         "operation":"add_multiplied_total",
         "amount":-1.00,
         "id":"quirks:fa_jin_explosive_release_disabled_movement"
      },
      "tick_rate":1,
      "condition":{
         "type":"origins:resource",
         "resource":"quirks:fa_jin/explosive_release_resource",
         "comparison":"==",
         "compare_to":1
      }
   },
   "disable_jump":{
      "type":"origins:modify_jump",
      "modifier":{
         "operation":"multiply_total_multiplicative",
         "amount":-1.00
      },
      "condition":{
         "type":"origins:resource",
         "resource":"quirks:fa_jin/explosive_release_resource",
         "comparison":"==",
         "compare_to":1
      }
   }
}
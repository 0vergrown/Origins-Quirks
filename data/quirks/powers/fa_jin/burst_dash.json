{
   "type":"origins:multiple",
   "energy_drain":{
      "type":"origins:action_over_time",
      "entity_action":{
         "type":"origins:change_resource",
         "resource":"quirks:fa_jin/kinetic_energy_resource",
         "change":-1,
         "operation":"add"
      },
      "interval":20,
      "condition":{
         "type":"origins:all_of",
         "conditions":[
            {
               "type":"origins:resource",
               "resource":"quirks:fa_jin/kinetic_energy_resource",
               "comparison":"!=",
               "compare_to":0
            },
            {
               "type":"origins:resource",
               "resource":"quirks:fa_jin/move_selector_resource",
               "comparison":"==",
               "compare_to":0
            },
            {
               "type":"origins:sprinting"
            }
         ]
      }
   },
   "effects":{
      "type":"origins:action_over_time",
      "entity_action":{
         "type":"origins:if_else",
         "condition":{
            "type":"origins:on_block"
         },
         "if_action":{
            "type":"origins:spawn_particles",
            "particle":{
               "type": "minecraft:dust",
               "params": "{color:[0.84,0.06,0.06],scale:1}"
            },
            "count":30,
            "speed":0.00,
            "force":false,
            "spread":{
               "x":0.30,
               "y":0.01,
               "z":0.30
            },
            "offset_x":0.00,
            "offset_y":0.30,
            "offset_z":0.00
         },
         "else_action":{
            "type":"origins:nothing"
         }
      },
      "rising_action":{
         "type":"origins:and",
         "actions":[
            {
               "type":"origins:play_sound",
               "sound":"minecraft:entity.generic.explode",
               "volume":1.00,
               "pitch":1.35
            },
            {
               "type":"origins:spawn_particles",
               "particle":"minecraft:gust",
               "count":6,
               "speed":0.50,
               "force":false,
               "spread":{
                  "x":0.50,
                  "y":0.50,
                  "z":0.50
               },
               "offset_x":0.00,
               "offset_y":0.50,
               "offset_z":0.00
            },
            {
               "type":"origins:spawn_particles",
               "particle":"minecraft:cloud",
               "count":12,
               "speed":0.10,
               "force":false,
               "spread":{
                  "x":1.50,
                  "y":1.50,
                  "z":1.50
               },
               "offset_x":0.00,
               "offset_y":0.50,
               "offset_z":0.00
            },
            {
               "type":"origins:area_of_effect",
               "radius":5.32,
               "shape":"cube",
               "bientity_action":{
                  "type":"origins:add_velocity",
                  "x":0.00,
                  "y":0.80,
                  "z":0.50,
                  "reference":"position",
                  "set":false
               },
               "bientity_condition":{
                  "type":"origins:target_condition",
                  "condition":{
                     "type":"origins:living"
                  }
               },
               "include_actor":false
            }
         ]
      },
      "interval":1,
      "condition":{
         "type":"origins:all_of",
         "conditions":[
            {
               "type":"origins:resource",
               "resource":"quirks:fa_jin/kinetic_energy_resource",
               "comparison":"!=",
               "compare_to":0
            },
            {
               "type":"origins:resource",
               "resource":"quirks:fa_jin/move_selector_resource",
               "comparison":"==",
               "compare_to":0
            },
            {
               "type":"origins:sprinting"
            }
         ]
      }
   },
   "speed":{
      "type":"origins:conditioned_attribute",
      "modifier":{
         "attribute":"minecraft:generic.movement_speed",
         "operation":"add_multiplied_total",
         "amount":1.00,
         "id":"quirks:fa_jin_burst_dash"
      },
      "tick_rate":1,
      "update_health":false,
      "condition":{
         "type":"origins:all_of",
         "conditions":[
            {
               "type":"origins:resource",
               "resource":"quirks:fa_jin/kinetic_energy_resource",
               "comparison":"!=",
               "compare_to":0
            },
            {
               "type":"origins:resource",
               "resource":"quirks:fa_jin/move_selector_resource",
               "comparison":"==",
               "compare_to":0
            },
            {
               "type":"origins:sprinting"
            }
         ]
      }
   }
}
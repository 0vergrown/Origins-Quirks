                                 "operation":"add"
                              },
                              {
                                 "type":"origins:play_sound",
                                 "sound":"quirks:move_slector.click",
                                 "volume":1.00,
                                 "pitch":1.00
                              }
                           ]
                        }
                     }
                  ]
               }
            },
            {
               "condition":{
                  "type":"origins:resource",
                  "resource":"quirks:power_reserve/kinetic_charge_power_output",
                  "comparison":"==",
                  "compare_to":5
               },
               "action":{
                  "type":"origins:and",
                  "actions":[
                     {
                        "type":"origins:change_resource",
                        "resource":"quirks:power_reserve/kinetic_charge_power_output",
                        "change":0,
                        "operation":"set"
                     },
                     {
                        "type":"origins:play_sound",
                        "sound":"quirks:move_slector.click",

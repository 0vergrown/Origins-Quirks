{
   "type":"origins:multiple",
   "resource":{
      "type":"origins:resource",
      "min":0,
      "max":1,
      "hud_render":{
         "should_render":false
      },
      "start_value":0
   },
   "phase_resource":{
      "type":"origins:resource",
      "min":0,
      "max":1,
      "hud_render":{
         "should_render":false
      },
      "start_value":0
   },
   "create_gate":{
      "type":"origins:action_over_time",
      "entity_action":{
         "type":"origins:execute_command",
         "command":"execute at @e[limit=1,sort=nearest,type=minecraft:area_effect_cloud,tag=all_for_one_warping_target_to_actor_gate] run particle minecraft:squid_ink ~ ~0.1 ~ 0.3 0.0 0.3 0 5 normal @a"
      },
      "rising_action":{
         "type":"origins:and",
         "actions":[
            {
               "type":"origins:execute_command",
               "command":"summon minecraft:area_effect_cloud ~ ~ ~ {Duration:100,Tags:[\"all_for_one_warping_target_to_actor_gate\"]}"
            },
            {
               "type":"origins:modify_resource",
               "modifier":{
                  "operation":"set_total",
                  "amount":1.00
               },
               "resource":"quirks:all_for_one/warping_teleport_target_to_actor_resource"
            },
            {
               "type":"origins:play_sound",
               "sound":"minecraft:entity.slime.squish",
               "volume":0.20,
               "pitch":1.00
            },
            {
               "type":"origins:play_sound",
               "sound":"minecraft:block.honey_block.slide",
               "volume":1.00,
               "pitch":0.50
            },
            {
               "type":"origins:delay",
               "action":{
                  "type":"origins:and",
                  "actions":[
                     {
                        "type":"origins:execute_command",
                        "command":"tp @s @e[limit=1,tag=all_for_one_warping_target_teleporting_to_actor,sort=nearest]"
                     },
                     {
                        "type":"origins:execute_command",
                        "command":"tp @s ~ ~-2 ~"
                     }
                  ]
               },
               "ticks":11
            },
            {
               "type":"origins:delay",
               "action":{
                  "type":"origins:modify_resource",
                  "modifier":{
                     "operation":"set_total",
                     "amount":0.00
                  },
                  "resource":"quirks:all_for_one/warping_teleport_target_to_actor_resource"
               },
               "ticks":12
            }
         ]
      },
      "interval":1
   },
   "phase":{
      "type":"origins:phasing",
      "blacklist":true,
      "block_condition":{
         "type":"origins:in_tag",
         "tag":"origins:unphasable"
      },
      "render_type":"blindness",
      "view_distance":10.00,
      "phase_down_condition":{
         "type":"origins:any_of",
         "conditions":[
            {
               "type":"origins:resource",
               "resource":"quirks:all_for_one/warping_teleport_target_to_actor_resource",
               "comparison":"==",
               "compare_to":1
            },
            {
               "type":"origins:resource",
               "resource":"quirks:all_for_one/warping_teleport_target_to_actor_phase_resource",
               "comparison":"==",
               "compare_to":1
            }
         ]
      },
      "condition":{
         "type":"origins:any_of",
         "conditions":[
            {
               "type":"origins:resource",
               "resource":"quirks:all_for_one/warping_teleport_target_to_actor_resource",
               "comparison":"==",
               "compare_to":1
            },
            {
               "type":"origins:resource",
               "resource":"quirks:all_for_one/warping_teleport_target_to_actor_phase_resource",
               "comparison":"==",
               "compare_to":1
            }
         ]
      }
   },
   "rise_from_ground":{
      "type":"origins:action_over_time",
      "entity_action":{
         "type":"origins:if_else",
         "condition":{
            "type":"origins:in_block",
            "block_condition":{
               "type":"origins:all_of",
               "conditions":[
                  {
                     "type":"origins:movement_blocking"
                  },
                  {
                     "type":"origins:in_tag",
                     "tag":"minecraft:beds",
                     "inverted":true
                  },
                  {
                     "type":"origins:in_tag",
                     "tag":"minecraft:doors",
                     "inverted":true
                  }
               ]
            }
         },
         "if_action":{
            "type":"origins:and",
            "actions":[
               {
                  "type":"origins:apply_effect",
                  "effect":{
                     "id":"minecraft:levitation",
                     "duration":2,
                     "amplifier":2,
                     "ambient":false,
                     "show_particles":false,
                     "show_icon":false
                  }
               },
               {
                  "type":"origins:modify_resource",
                  "modifier":{
                     "operation":"set_total",
                     "amount":1.00
                  },
                  "resource":"quirks:all_for_one/warping_teleport_target_to_actor_phase_resource"
               }
            ]
         },
         "else_action":{
            "type": "origins:and",
            "actions": [
               {
                  "type":"origins:modify_resource",
                  "modifier":{
                     "operation":"set_total",
                     "amount":0.00
                  },
                  "resource":"quirks:all_for_one/warping_teleport_target_to_actor_phase_resource"
               },
               {
                  "type": "origins:clear_effect",
                  "effect": "minecraft:levitation"
               }
            ]
         }
      },
      "rising_action":{
         "type":"origins:and",
         "actions":[
            {
               "type":"origins:play_sound",
               "sound":"minecraft:entity.slime.squish",
               "volume":0.20,
               "pitch":0.50
            },
            {
               "type":"origins:play_sound",
               "sound":"minecraft:block.honey_block.slide",
               "volume":1.00,
               "pitch":0.50
            }
         ]
      },
      "interval":1,
      "condition":{
         "type":"origins:resource",
         "resource":"quirks:all_for_one/warping_teleport_target_to_actor_resource",
         "comparison":"==",
         "compare_to":0
      }
   },
   "slow_fall":{
      "type":"origins:modify_falling",
      "velocity":0.01,
      "take_fall_damage":false,
      "condition":{
         "type":"origins:all_of",
         "conditions":[
            {
               "type":"origins:resource",
               "resource":"quirks:all_for_one/warping_teleport_target_to_actor_resource",
               "comparison":"==",
               "compare_to":1
            },
            {
               "type":"origins:in_block",
               "block_condition":{
                  "type":"origins:movement_blocking"
               }
            }
         ]
      }
   },
   "suffocate_invulnerability":{
      "type":"origins:invulnerability",
      "damage_condition":{
         "type":"origins:name",
         "name":"inWall"
      }
   }
}
{
   "type":"origins:multiple",
   "target_list":{
      "type":"origins:entity_set",
      "action_on_remove":{
         "type":"origins:target_action",
         "action":{
            "type":"origins:and",
            "actions":[
               {
                  "type":"origins:clear_effect",
                  "effect":"minecraft:nausea"
               },
               {
                  "type":"origins:clear_effect",
                  "effect":"minecraft:slowness"
               }
            ]
         }
      }
   },
   "active":{
      "type":"origins:active_self",
      "entity_action":{
         "type":"origins:if_else",
         "condition":{
            "type":"origins:sneaking"
         },
         "if_action":{
            "type":"origins:action_on_entity_set",
            "set":"quirks:sloshed/targeted_slosh_target_list",
            "bientity_action":{
               "type":"origins:remove_from_entity_set",
               "set":"quirks:sloshed/targeted_slosh_target_list"
            },
            "limit":1,
            "reverse":false
         },
         "else_action":{
            "type":"origins:raycast",
            "distance":15.00,
            "block":false,
            "entity":true,
            "shape_type":"visual",
            "fluid_handling":"none",
            "bientity_condition":{
               "type":"origins:in_entity_set",
               "set":"quirks:sloshed/targeted_slosh_target_list",
               "inverted":true
            },
            "bientity_action":{
               "type":"origins:add_to_entity_set",
               "set":"quirks:sloshed/targeted_slosh_target_list"
            },
            "entity_distance":15.00,
            "command_step":1.00,
            "command_along_ray_only_on_hit":false
         }
      },
      "cooldown":1200,
      "hud_render":{
         "should_render":true,
         "sprite_location":"origins:textures/gui/resource_bar.png",
         "bar_index":5
      },
      "key":{
         "key":"key.origins.primary_active",
         "continuous":false
      },
      "condition":{
         "type":"origins:resource",
         "resource":"quirks:sloshed/move_selector_resource",
         "comparison":"==",
         "compare_to":1
      }
   },
   "apply_effect_to_set":{
      "type":"origins:action_over_time",
      "entity_action":{
         "type":"origins:action_on_entity_set",
         "set":"quirks:sloshed/targeted_slosh_target_list",
         "bientity_action":{
            "type":"origins:target_action",
            "action":{
               "type":"origins:and",
               "actions":[
                  {
                     "type":"origins:apply_effect",
                     "effects":[
                        {
                           "id":"minecraft:nausea",
                           "duration":100,
                           "amplifier":0,
                           "ambient":false,
                           "show_particles":true,
                           "show_icon":true
                        },
                        {
                           "id":"minecraft:slowness",
                           "duration":100,
                           "amplifier":2,
                           "ambient":false,
                           "show_particles":true,
                           "show_icon":true
                        }
                     ]
                  },
                  {
                     "type":"origins:spawn_particles",
                     "particle":{
                        "type":"minecraft:dust",
                        "params":"{color:[0.4,1.0,0.5],scale:1.0}"
                     },
                     "count":1,
                     "speed":1.00,
                     "force":true,
                     "spread":{
                        "x":0.15,
                        "y":0.17,
                        "z":0.15
                     },
                     "offset_x":0.00,
                     "offset_y":1.50,
                     "offset_z":0.00
                  }
               ]
            }
         },
         "bientity_condition":{
            "type":"origins:distance",
            "comparison":"<=",
            "compare_to":15
         },
         "limit":1,
         "reverse":false
      },
      "interval":20,
      "condition":{
         "type":"origins:entity_set_size",
         "set":"quirks:sloshed/targeted_slosh_target_list",
         "comparison":"!=",
         "compare_to":0
      }
   },
   "glow":{
      "type":"origins:entity_glow",
      "bientity_condition":{
         "type":"origins:all_of",
         "conditions":[
            {
               "type":"origins:in_entity_set",
               "set":"quirks:sloshed/targeted_slosh_target_list"
            },
            {
               "type":"origins:can_see",
               "shape_type":"visual",
               "fluid_handling":"none"
            }
         ]
      },
      "use_teams": false,
      "red": 1.00,
      "green": 1.00,
      "blue": 1.00
   }
}